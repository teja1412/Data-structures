DoublyLinkedListNode* reverse(DoublyLinkedListNode* llist){
    DoublyLinkedListNode* temp=llist;
    DoublyLinkedListNode* curr;
    DoublyLinkedListNode* previous;
    int c=0;
    previous=llist;
    curr=llist;
    int list_length(DoublyLinkedListNode* temp)
    {
        while(temp->next!=NULL)
        {
            c++;
            temp=temp->next;
        }
        return c;
    }
    temp=llist;
    int l,i,j;
    l=list_length(temp);
    for(i=0;i<=l/2;i++)
    {
        for(j=0;j<l-i;j++)
        {
            curr=curr->next;
        }
        int temp;
        temp=curr->data;
        curr->data=previous->data;
        previous->data=temp;
        previous=previous->next;
        curr=llist;
    }
    return llist;
}

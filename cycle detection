bool has_cycle(SinglyLinkedListNode* head){
    if (head == NULL){
        return 0;
    }
    SinglyLinkedListNode* first;
    SinglyLinkedListNode* second;
    first = head;
    second= head;

    while (second != NULL && second->next != NULL){
        first =first->next;
        second=second->next->next;

        if (first == second){
            return 1;
        }
    }

    return 0;
}

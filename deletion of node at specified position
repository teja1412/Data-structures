SinglyLinkedListNode* deleteNode(SinglyLinkedListNode* llist, int position){
    SinglyLinkedListNode* t;
    int i;
    t=llist;
    if(t==NULL)
    {
        return t;
    }
    if(position==0)
    {
        t=t->next;
        return t;
    }
    for(i=0;i<position-1;i++)
    {
        t=t->next;
    }
    if(t->next)
    {
        t->next=t->next->next;
        return llist;
    }
    t->next=NULL;
    return llist;
}
